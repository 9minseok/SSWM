pipeline {
    agent any

//    environment {
        // Define any environment variables you might need during the build and deployment
//        NODE_ENV = 'production'
//    }

    stages {
        stage('Checkout') {
            steps {
                git url: 'https://lab.ssafy.com/s09-webmobile1-sub2/S09P12A206', branch:'deploy-frontend'
                dir('sswm-front'){
                    sh 'pwd'
                } 
            }
        }

    }

    post {
        success {
            // This block will be executed if the pipeline succeeds
            echo 'Build and deployment succeeded!'
        }

        failure {
            // This block will be executed if the pipeline fails
            echo 'Build or deployment failed!'
        }
    }
}
